generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  id          String   @id @default(cuid())
  number      Int
  serviceType String
  status      String   @default("WAITING")
  createdAt   DateTime @default(now())
  qrCodeData  String?

  @@map("tokens")
}